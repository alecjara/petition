<h1>List of signers</h1>

{{#each signers}}
<!-- for part4 do something similar to this: -->
    {{#if url}}
    <p><a href={{url}}>{{firstname}} {{lastname}}</a> {{age}} <a href="/signers/{{city}}">{{city}}</a></p>
    {{else}}
    <p>{{firstname}} {{lastname}} {{age}} <a href="/signers/{{city}}">{{city}}</a></p>
    {{/if}}
{{/each}}

{{#if link}}
    <a href="/signers">Click to see the list of signers.</a>
{{/if}}

